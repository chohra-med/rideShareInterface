{
  "name": "rideShareInterface",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "debug": "open rndebugger://set-debugger-loc?host=localhost&port=8081",
    "link-ios": "cd ios/ && pod install && cd ..",
